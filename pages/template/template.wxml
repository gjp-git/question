<template name="userRank">
  <view  class='userGrade'>
    <image class='avatar' src="{{avatar}}"></image>
    <image wx:if='{{canUse}}' wx:if="{{userRank!=''}}" class='rank' src="https://54.gmair.net/material/kun{{userRank}}.jpg"></image>
    <image wx:if='{{canUse}}' wx:if="{{userRank!=''}}" class='star' src="https://54.gmair.net/material/{{userStar}}star.png"></image>
  </view>
</template>